    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.30",
    "grpc-tools": "^1.13.0",
    "jest": "^30.0.3",
    "postcss": "^8.4.34",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.4",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3",
    "turbo": "^1.13.4",
    "typescript": "^5.8.3"
