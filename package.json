{
  "name": "vanity-addy-generator",
  "version": "1.0.0",
  "description": "Solana Vanity Address Generator with GPU Acceleration",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "test": "jest",
    "dev": "turbo dev",
    "build": "turbo build",
    "start": "turbo start",
    "lint": "turbo lint",
    "generate-proto": "mkdir -p generated/python && grpc_tools_node_protoc --proto_path=proto --python_out=generated/python --grpc_python_out=generated/python proto/vanity.proto",
    "deploy:anchor": "tsx scripts/deploy/anchor.ts",
    "deploy:runpod": "tsx scripts/deploy/runpod.ts",
    "gpu:build": "cd packages/workers/cuda && bash build.sh",
    "prebuild": "pnpm generate-proto",
    "postinstall": "pnpm generate-proto"
  },
  "keywords": [
    "solana",
    "vanity",
    "gpu",
    "cuda",
    "web3"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
